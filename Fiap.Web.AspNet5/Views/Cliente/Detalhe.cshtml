@model ClienteModel

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="well">
                <h2>Cliente - Editar</h2>

                <div class="form-group">
                    <label class="control-label">Nome</label>
                    <input class="form-control col-5" readonly name="Nome" value="@Model.Nome" />
                </div>

                <div class="form-group">
                    <label class="control-label">Email</label>
                    <input class="form-control col-5" readonly name="Email" value="@Model.Email" />
                </div>

                <div class="form-group">
                    <label class="control-label">Data Nascimento</label>
                    <input class="form-control col-5" readonly name="DataNascimento" value="@Model.DataNascimento" />
                </div>

                <div class="form-group">
                    <label class="control-label">Observação</label>
                    <textarea class="form-control" readonly name="Observacao">@Model.Observacao</textarea>
                </div>

                <a href="#" class="btn btn-default btn-lg">Voltar</a>
    
            </div>
        </div>
    </div>
</div>